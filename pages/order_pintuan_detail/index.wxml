    <view class='wuliuItem' wx:for='{{orderDetailData.kuaidiBean.data}}' wx:key='{{index}}'>
      <view style="color:#333">{{item.context}}</view>
      <view class='time'>{{item.time}} </view>
    </view>
  </view> 


  
