.orderTime2 text{
  padding: 2px;
}



/*拼团  */
.shop_info{
  display: flex;
  align-items: center;
}
.pintuan_info{
  display: flex;
  flex-direction: row;
  align-items: center;
}
.pintuan_bg{
  display: flex;
  position: relative;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 4rpx;
  padding: 0 50rpx;
  height: 190rpx;
}
.pintuan_bg_right{
  margin-left: 60rpx;
}
.share_btn{
  position: absolute;
  right: -10rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  padding:10rpx 20rpx;
  color: #888;
  background: #fff;
  border-radius: 10rpx;
  box-shadow:6rpx 6rpx 10rpx #555;
  height:40rpx;
  top:10%;
}
.share_icon{
  width: 30rpx;
  height: 30rpx;
  margin-right: 10rpx;
}
.share_text{
  font-size: 22rpx;
}
.pintuan_bg_left{
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  line-height:40rpx;
}
.pintuan_bg_right_img{
  width:150rpx;
  height:120rpx;
}
.pintuan_container{
  display: flex;
  position: relative;
  flex-direction: column;
  align-items: flex-start;
  justify-content: center;
  color: #888;
  background: #fff;
  border-radius: 0.2em;
  padding:1em 1em 0 1em;
  margin-bottom: 10rpx;
}
.pintuan_container .pintuan_tiltle{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start;
  height: 60rpx;
  width:100%;
  font-size: 28rpx;
}
.pintuan_user_img{
  width: 35rpx;
  height: 35rpx;
  margin-right: 20rpx;
}
.pintuan_container .pintuan_users{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  padding-left: 60rpx;
  box-sizing: border-box;
  /* height: 140rpx; */
  width: 100%;
  margin:20rpx 0;
}
.pintuan_user_list{
  display: flex;
  align-items: center;
  width:82%;
  flex-wrap: wrap;
}
.pintuan_user_item{
  border: 2rpx solid #e5e600;
  border-radius: 100rpx;
  width: 90rpx;
  height: 90rpx;
  margin-right: 15rpx;
  position: relative;
  margin-bottom:10rpx;
}
.user_head_img{
  width: 90rpx;
  height: 90rpx;
  border-radius: 90rpx;
}
.pintuan_user_list_tip{
  border: 4rpx solid #fff;
  width: 50rpx;
  height: 30rpx;
  border-radius: 30rpx;
  position: absolute;
  top:-4rpx;
  left:-6rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 20rpx;
  color: #fff;
  line-height:30rpx;

}
.pintuan_invitation_button{
  border-radius:10rpx;
  height:50rpx;
  width:18%;
  padding:10rpx 20rpx;
  display:flex;
  flex-direction:row;
  align-items:center;
  justify-content:center;
  font-size:26rpx;
  color:#fff;
  margin:0;
  line-height:50rpx;
  box-sizing:content-box;
}
.unpintuan_user_item{
  border: 2rpx dashed #999;
  border-radius: 90rpx;
  width: 90rpx;
  height: 90rpx;
  margin-right: 15rpx;
  display: flex;align-items: center;
  justify-content: center;
  margin-bottom:10rpx;
}
.unuser_head_img{
  width: 40rpx;
  height: 40rpx;
}