   /* typesearch */
.typeSearchContainer1{
  position:fixed;
  z-index:111;
  width: 100%;
  height: 150rpx;

}
.search_input_bg{
  padding: 20rpx;
  background: #fff;
  width: 100%;
  height: 80rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  box-sizing:border-box;
}
.search_input{
  border-radius: 60rpx;
  height: 60rpx;
  background: #eee;
  width: 100%;
  padding: 15rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between
}
.search_input_left{
  display: flex;
  flex-direction: row;
  align-items: center;
}
.search_input .input{
  font-size: 24rpx;
  margin-left: 20rpx;
}
.input_img{
  width: 35rpx;
  height: 35rpx;
}
.empty_button{
  color: #fff;
  background: #999;
  width: 30rpx;
  height: 30rpx;
  font-size: 22rpx;
  border-radius: 30rpx;
  line-height: 30rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  margin-right: 10rpx;
  opacity: 0.8;
  cursor: pointer;
}
.typeSearch{
  position: fixed;
  z-index:111;
  top: 80rpx;
  width: 100%;
  height: 3em;
  background: #fff;
  display: flex;
  flex-direction: row;
  border-bottom: 2rpx solid #ccc;
}
.typeSearch > view{
  width: 25%;
  box-sizing: border-box;
  font-size: small;
  padding: 1em 0;
  position: relative;
}
.border_right{
  position: absolute;
  right: 0;
  border-right: 1rpx solid #ccc;
  top: 20%;
  bottom: 20%;
}
.typeSearch image{
  height: 1em;
  width: 1em;
}
.typeSearch_view_height{
  height: 3em;
  background: #f1f1f1;
}


 /*  */
.smoprice{
  font-size: xx-small;
  color: red;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}

/*分类弹出来的样式*/
.typeSearch_item_container{
  position: fixed;
  top: 3em;
  left: 0;
  right: 0;
  height: 60%;
  width: 100%;
  z-index: 110;
  background: #fff;
}

 .typeSearch_item_left_container{
  position: absolute;
  left: 0;
  top: 0;
  bottom: 0;
  height: 100%;
  right: 50%;
  overflow-x: hidden;
  overflow-y: scroll;
  
} 

.searchPrice{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
}

