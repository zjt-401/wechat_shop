}
.price{
  font-size: 38rpx;
  height:38rpx;
  line-height:38rpx;
  color: #ff826e
}
.to_car{
  position: absolute;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  bottom: 20rpx;
  right: 20rpx;
  border-radius: 24rpx;
  font-size: 20rpx;
  border: 1rpx solid #ff0000;
  color: #ff0000;
  padding: 6rpx 18rpx;
}