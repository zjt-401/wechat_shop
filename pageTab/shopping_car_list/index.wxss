/* pages/cart/index.wxss */


/* @import '../../view-new/aikucun/aikucun_hot_product.wxss'; */
@import '../../view-new/aikucun/aikucun_title.wxss';/*  detail_list_card  */ 
 @import '../../view-new/aikucun/measure_card.wxss'; 
 @import '../../view-new/aikucun/kefu.wxss'; 


.hasMoreCart{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  height: 3em;
  background: #eee;
  font-size: 0.8em;
}
.container{ 
    background: #f3f3f3;
    font-size: 0.8em;
    
  min-height: 10em;
}
/* 购物车列表 */
.cartContainer{
  width: 100%;
  display: flex;
  flex-direction: column;
  color: #888;
  background: #fff;
}
.cartShopName{
  padding: 15px 10px;
  text-align: left;
  border-bottom: 1px solid #dddddd; 
}

.cartItems{
  display: flex;
  flex-direction: row;
  padding: 10px 0 10px 10px;
  overflow: hidden;
  border-bottom: 1px solid #dddddd;
  /* height: 6em; */
  position: relative;
   align-items: center;
}
.itemCheckbox{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  padding-right:5px;
}
.itemCheckbox_image{
  width: 6em;
  height: 6em;
  border: 1rpx solid #eee;
}

.item_infomation{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding-left:10px;
  min-height:6em;
}
.item_infomation_name{
  width: 10em;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  color: #000;
}
.cart_stock_warn{
  font-size: 0.8em;
  color: #aaa;
}
.cart_infoMeasure{
  width: 10em;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  color: #999;
  word-break: break-all;
}
.cart_infoMeasure view{
  margin-right: 10px;
}

.cart_star{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  word-break: break-all;
}
.cart_star > view{
  margin-right: 10px;
  background: #7fdb8a;
  padding: 0 0.4em;
  border-radius: 0.4em;
  color: #fff;
  max-width:10em;
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis;

}
.item_infomation_money{
  font-weight: bold;
  color: red;
}

.item_infomation_nums_cart_not_add{
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  align-items: center;
  position: absolute;
  right: 2em;
  bottom: 1em;
}
.item_infomation_nums_cart_not_add > .x_butn{
  margin-right: 0.2em;
}

/* 加和减 */
.item_infomation_nums_cart{
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  align-items: center;
  position: absolute;
  right: 2em;
  bottom: 10px;
}

.item_infomation_nums_cart image{
  height: 1.2em;
  width: 1.2em;
  padding: 5px;
  border: 1rpx solid #aaa;
}
.item_infomation_nums_cart text{
  height: 1.2em;
  width: 1.2em;
  padding: 5px;
  border-top: 1rpx solid #aaa;
  border-bottom: 1rpx solid #aaa;
}

.dellCart_butn{
  position: absolute;
  top: 0.4em;
  right:1em;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.xxx_edit{
  padding: 0.6em;
}
.xxx_edit > image{
  width: 1em;
  height: 1em;
}
.xxx{
  padding: 0.4em;
}
.xxx > image{
  width: 1em;
  height: 1em;
}
/* 删除 */

.butn_container{
  padding: 10px;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  border-bottom: 1em #eee solid;
}
.butn_container view{
  width: 7em;
  height: 2.6em;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #fff;
  color: aaa;
  border: 1rpx solid silver;
  font-size: 0.8em;
  margin-right: 10px;
}

/* 操作订单加减 */

.editCart_item_container{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  margin-left: 10px;
  padding: 0.6em 0;
  align-items:center;

}
.dellCart_butn_container{
  display: flex;
  flex-direction: column;
  align-items:center;
  width:8rem;

  height: 100%;
  justify-content:space-between;
}
.dellCart_butn_container_1{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content:center;
  width:100%;

}

.dellCart_butn_container_1 > image{
  height: 1.2em;
  width: 1.2em;
  padding: 0.4em;
  border: 1rpx solid #aaa;
  border-radius: 0.2em;
}
.dellCart_butn_container_1 > text{
  height: 1.2em;
  width: 2.6em;
  padding: 0.4em;
  border: 1rpx solid #aaa;
  margin-left: 0.4em;
  margin-right: 0.4em;
  border-radius: 0.2em;
}
.dellCart_butn_container_mesure{
  display: flex;
  flex-direction: row;
  align-items: center;
  border-radius:0.2em;
  width:90%;
  margin-top:0.4em;
  padding:0.2em 5%;
  flex-wrap:wrap;

}
.cantAdd{
  background: #eee;
}
.dellCart_butn_container_mesure > text{
  margin-right: 0.4em;
  color: #aaa;
}
.sureCart_butn_container{
  width: 20%;
  height: 50%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  color: #fff;
  background: #eee;
  position:absolute;
  right:0;
  top: 25%;
  bottom: 25%;
}


/* 底部下单按钮 */
.cart_buttomHeight{
  height: 4em;
}
.cart_buttom{
  position: fixed;
  z-index: 5;
  bottom: 0;
  width: 100%;
  height: 3em;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  background:#eeeeee;
}
.chooseAll{
  font-size: x-small;
  padding-left: 1em;
}
.chooseAll checkbox{
  background: #eee
}
.toJieSuan{
  display: flex;
  flex-direction: row;
  align-items: center;
}
.toJieSuan_money{
  height: 3em;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: flex-end;
  color: grey;
  font-size: 0.8em;
  padding-right: 5px;
}
.toJieSuan_butn{
    height: 3em;
    font-weight: bold;
    text-align: center;
    width: 6em;
    background: #8B572A;
    color: white;
    display: flex;
    justify-content: center;
    align-items: center;
}
/* none */
.noCart{
  
  color: #555;
  background: #f2f2f2;
  height: 15em;
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.noCart image{
  width: 25%;
}
.noCart text{
  margin-top: 20px;
}



/* 选择活动的卡片 */
.kefu_wx_huodong{
  position:fixed;
  top:0;
  left:0;
  right:0;
  bottom:0;
  z-index:11;
  display:flex;
  flex-direction:column;
  align-items:center;
  justify-content:center;
  width: 100%;
  height: 100%;
}
.kefu_bg_zhezhao{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: #000;
  opacity: 0.6;
  z-index: 12;
}

.kefu_wx_card_hongdong{
  position: fixed;
  left:15%;
  right:15%;
  top: 30%;
  background: #fff;
  z-index: 13;
  font-size: 0.8em;
  border-radius: 0.4em;
  overflow: hidden;

}
.kefu_wx_card_hongdong > .name{
  padding: 1.6em;
  font-size: 1em;

}

.code_container{
  display: flex;
  flex-direction: column;
  padding: 0.2em 1.6em;
}
.radioItems{
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-bottom: 0.8em;
  word-break: break-all;
}
.radioItems > radio{
  margin-right: 0.8em;
}

.tips_buy_butn_container{
  display: flex;
  flex-direction: row;
  width: 100%;
  border-top: 1rpx solid #ccc;
  height: 3.6em;
}
.code_item{
  
  word-break: break-all; 
}
.tips_buy_butn_container > view{
  width: 50%;
  height:100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
  
.backtoCart_butn{
  background: #fff;
  color: #333;
}
.go_to_pay_butn{
background: red;
  color: #fff;
}

/* 热销商品 */
.hotProductContainer{
  padding:  10px;
  background: #f9f9f9;
}