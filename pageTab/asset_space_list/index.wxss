     color: #999;
     height: 76rpx;
   }
   .navigation{
     display: flex;
     flex-direction: row;
     align-items: center;
     height: 50rpx;
     border-radius: 12rpx;
     font-size: 24rpx;
     color: #fff;
     width:100rpx;
     padding: 0 10rpx
   }
   .navigation_icon{
     width: 26rpx;
     height: 26rpx;
     margin-right: 10rpx;
   }
   .distance{
     padding:0 10rpx;
     border-radius:10rpx;
     margin:0 6rpx;
   }