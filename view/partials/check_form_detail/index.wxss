}
.havePayUserBlock{
  display: flex;
  flex-direction: column;
  align-items: center;
  padding:20rpx;
  background:#f8f8f8f8;
  border-radius:16rpx;
  margin:0 20rpx;
}
.havePayUserTitle{
  font-size: 28rpx;
  margin-bottom: 20rpx;
  font-weight: bold;
}
.havePayUserList{
  display: flex;
  flex-direction: row;
  align-items: center;
  flex-wrap: wrap;
}
.havePayUserItem{
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-right: 16rpx;
  margin-bottom: 20rpx
}
.userIcon{
  margin-bottom: 16rpx;
  width: 80rpx;
  height: 80rpx;
  border-radius: 80rpx;
}
.userName{
  font-size: 20rpx;
  width: 120rpx;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
}
.titleStyle{
  display: flex;
  flex-direction: row;
  align-items: center;
}
.tags_item{
  border: 2rpx solid #eee;
  padding: 2rpx 12rpx;
  font-size: 22rpx;
  color: #888;
  margin-right: 10rpx;
  border-radius: 10rpx;
  white-space:nowrap;
  box-sizing: content-box;
  display:inline-block;
}
.tags_content{
  display: flex;
  flex-direction: row;
  align-items: center;
  overflow-x:hidden;
  width:100%;
  height:40rpx;
  white-space: nowrap
}
.center_part_item_title{
  white-space:nowrap;
}
.center_part_item_value{
  text-align: justify;
  text-justify: newspaper;
  word-break: break-all;
}