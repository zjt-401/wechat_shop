.content{
  display: flex;
  flex-direction: column;
  align-items: center;
  background: #f5f5f5
}
.formBanner{
  width: 100%;
  padding: 20rpx;
  box-sizing:border-box;
  background:#fff;
}
.bannerUrl{
  width: 100%;
  height: 100%
}
.formHead{
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: 28rpx;
  padding: 20rpx;
  width: 100%;
  box-sizing: border-box;
}
.formName{
  padding:0 0 20rpx 0;
  font-size: 34rpx;
  font-weight: bold;
  letter-spacing: 2rpx;
  color:#444444;
}
.formDescription{
  letter-spacing: 2rpx;
  line-height:32rpx;
  font-size:22rpx;
  display: flex;
  flex-direction: row;
  align-items: flex-start;
  color: #888;
  width: 100%
}
.tip{
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-right: 20rpx;
}
.tipIcon{
  width: 32rpx;
  height: 32rpx;
  margin-right: 6rpx
}
.tipText{
  width: 100%;
}
.formContent{
  display: flex;
  flex-direction: column;
  justify-content: center;
  width:100%;
  padding: 20rpx;
  box-sizing: border-box;
}
.common{
  background: #fff;
  border-radius: 6rpx;
  min-height: 80rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 0 18rpx;
  margin: 2rpx 0;
  justify-content: space-between;
  font-size:28rpx;
  position: relative;
  z-index: 1

}
.inputName{
  font-size: 28rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.inputString input{
  text-align: right;
  width: 60%
}
.inputString textarea {
  text-align:left;
  padding: 10rpx;
  width:96%;
  border:1rpx solid #eee;
  margin-top:10rpx;
  height: 200rpx;
  position: relative;
  z-index: 1;
}

/* .picker{
  width:60%;
  height:100%;
  line-height:76rpx;
} */
.pickerRight{
  height:80rpx;
  display:flex;
  align-items:center;
  width:40rpx;
  float:right;
}
.pickerValue{
  width:90%;
  height:80rpx;
  float: left;
  display: flex;
  flex-direction: row;
  justify-content: flex-end
  
}
.pickerImg{
  width: 50rpx;
  float: right;
  height:50rpx;
}
.imgType{
  flex-direction: column;
  align-items: flex-start;
  background: #fff; 
  height:100%;
  font-size:28rpx;
}
.imgName{
  margin-top: 15rpx
}
.upload{
  display: flex;
  flex-direction: row;
  align-items: center;
  flex-flow: wrap;
  margin-top:15rpx;
}
.uploadImg{
  width: 150rpx;
  height: 150rpx;
  border:2rpx solid #888;
  display: flex;
  flex-direction: column;
  align-items:center;
  justify-content:center;
  margin-bottom: 15rpx
}
.uploadImgIcon{
  width: 50rpx;
  height: 50rpx
}
.uploadedImgUrl{
  width: 150rpx;
  height: 150rpx;
  margin-right: 20rpx;
  border-radius: 10rpx
}
.uploadedImg{
  margin-bottom: 15rpx;
  position: relative;
}
.removeImgBg{
  width:150rpx;
  height: 40rpx;
  background: #888;
  opacity: 0.5;
  position: absolute;
}
.removeImgIcon{
  position: absolute;
  right: 26rpx;
  top:6rpx;
  border-radius: 30rpx;
  width: 30rpx;
  height: 30rpx;
  background: #888;
  font-size: 20rpx;
  text-align:center;
  color: #fff
}
.saveData{
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between
}
.picker{
  width: 400rpx;
  height: 80rpx;
  line-height:76rpx;
  text-align:right;
  float:left;

}
.saveBtn{
  width: 100%
}
/* 信息发布样式 */
.content2{
    display: flex;
    flex-direction: column;
    background: #fff;
    width: 100%;
}
.form_detail_top{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx 30rpx;
  width: 100%;
  box-sizing: border-box;
}
.form_detail_top_left{
  display: flex;
  flex-direction: row;
  align-items: center;
}
.user_head_img{
  width: 100rpx;
  height: 100rpx;
  margin-right: 20rpx;
  border-radius: 100rpx;
}
.user_base_info{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  height: 80rpx;
}
.user_base_name{
  font-size: 26rpx;
  color: #888;
}
.release_time{
  font-size: 22rpx;
  color: #ccc;
}
.telno_icon{
  width: 50rpx;
  height: 50rpx;
}
/* center */
.form_detail_center{
  display: flex;
  flex-direction: column;
  padding: 15rpx 0;
  margin: 0 30rpx;
  border-top: 1rpx solid #ddd;
  color: #888;
  position: relative;
  font-size: 26rpx;
}
.form_detail_center_item{
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-bottom: 10rpx;
  position: relative;
  box-sizing:border-box;
}
.interval{
  margin: 0 10rpx 0 4rpx;
}
.read_number{
  position: absolute;
  top: 15rpx;
  right: 0;
  padding: 4rpx 8rpx;
  color: #ccc;
  font-size: 18rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  border: 1rpx solid #ccc;
  border-radius:10rpx;
}
.form_detail_center_arr{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.form_detail_center_arr_img{
  width: 100%;
  height:500rpx;
  margin-bottom:10rpx;
}
.map_location{
  width: 100%;
  height: 220rpx;
  position: relative;
}
.mapItem{
  position: relative;
  z-index: 1;
}
.map_bg{
  width: 100%;
  height: 220rpx;
  position: absolute;
  left: 0;
  top: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.play{
  height: 60rpx;
  background: #fff;
  padding: 2rpx 20rpx;
  color: #888;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  border-radius: 10rpx;
  font-size: 26rpx;
  
}
.to_addr_icon{
  margin-left: 20rpx;
  width: 30rpx;
  height: 30rpx;
}
.sanjiao{
    width: 30rpx;
    height: 30rpx;
}
.form_detail_bottom{
  display: flex;
  flex-direction: column;
  border-top: 10rpx solid #ddd;
  margin-top:20rpx;
  margin-bottom: 100rpx;
}
.form_detail_bottom_top{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx 30rpx;
  border-bottom: 1rpx solid #ddd;
}
.bottom_top_item{
  display: flex;
  flex-direction: row;
  align-items: center;
}
.bottom_top_item_icon{
  width: 40rpx; 
  height: 40rpx;
  margin-right: 20rpx;
}
.bottom_top_item_text{
  font-size: 24rpx;
  color: #888;
}
.comments_list{
  display: flex;
  flex-direction: column;
  padding: 0 30rpx 30rpx 30rpx;
}
.comments_item{
  border-bottom: 1rpx solid #ddd;
  display: flex;
  flex-direction: row;
  align-items: flex-start;
  font-size: 24rpx;
  padding:10px;
}
.user_img{
  width: 80rpx;
  height: 80rpx;
  margin-right: 20rpx;
  border-radius: 80rpx;
}
.comments_item_right{
  display: flex;
  flex-direction: column;
  width:86%;
}
.user_name{
  color: #888;
  margin-bottom: 6rpx
}
.user_need{
  color: #666;
  margin-bottom: 6rpx
}
.other_reply_item{
  display: flex;
  flex-direction: row;
  margin-bottom: 6rpx;
  background: #f1f1f1;
  border-radius: 6rpx;
  padding: 4rpx 8rpx 4rpx 0;
}
.other_reply_name{
  color: #1f98ec;
}
.other_reply_text{
  color: #666;
}
.more_reply{
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
  color: #1f98ec;
  padding:10rpx 0;
}
.comments_item_bottom{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  color: #ccc;
}
.comments_icon{
  width: 40rpx;
  height: 40rpx;
}
.comments_part{
  position: fixed;
  bottom: 0;
  height: 100rpx;
  width: 100%;
  background: #f1f1f1;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.comments_input{
  width:610rpx;
  height:60rpx;
  border-radius:10rpx;
  background: #fff;
  padding: 6rpx 10rpx;
  font-size: 26rpx;
}
.comments_img_icon{
  width: 60rpx;
  height: 60rpx;
}
.no_item_reply{
  display: flex;
  align-items: center;
  justify-content: center;
  background: #f7f7f7;
  color: #999;
  font-size: 26rpx;
  padding: 20rpx;
}
.to_navigation{
  padding:4rpx 10rpx;
  border-radius:10rpx;
  color:#fff;
  font-size:22rpx;
  margin-left:20rpx;
  display:flex;
  align-items:center;
}
.interval{
  display: flex;
  align-items: center;
  padding-left:5px;
}
.interval_banner{
  width: 100%;
  /* height: 10rpx; */
  display: block;
}
.select_address {
overflow:hidden;
text-overflow:ellipsis;
white-space:nowrap;
}
.copyText{
  position: absolute;
  bottom: 30rpx;
  right: 34rpx;
  padding: 4rpx 10rpx;
  border: 1rpx solid #ccc;
  border-radius: 10rpx;
  background: #fff;
  color: #999;
  font-size:24rpx;
  z-index: 2;
}
.bottomBtn{
  position: fixed;
  bottom: 0;
  height: 100rpx;
  width: 100%;
  /* background: #f1f1f1; */
  border-top: 2rpx solid #ddd ;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  padding: 0 20rpx;
  box-sizing: border-box;
  background:#fff;
}
.bottomBtnTel{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
  border-radius:50rpx;
  padding:16rpx 30rpx;
  box-sizing: border-box;
}
.bottomBtnForm{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
  border-radius:50rpx;
  padding:16rpx 30rpx;
  box-sizing: border-box;
}
.bottomBtnPay{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
  border-radius:50rpx;
  padding:16rpx 30rpx;
  box-sizing: border-box;
  color: #fff
}
.w32per{
  width:32%
}
.w48per{
