
.titleStyle{
  display: flex;
  flex-direction: row;
  align-items: center;
}
.tags_item{
  border: 2rpx solid #eee;
  padding: 2rpx 12rpx;
  font-size: 22rpx;
  color: #888;
  margin-right: 10rpx;
  border-radius: 10rpx;
  white-space:nowrap;
  box-sizing: content-box;
  display:inline-block;
}
.tags_content{
  display: flex;
  flex-direction: row;
  align-items: center;
  overflow-x:hidden;
  width:100%;
  height:40rpx;
  white-space: nowrap
}
.to_navigation{
  border-radius:20rpx;
  font-size:26rpx;
  padding:6rpx 20rpx;
  margin-left:10rpx;
}