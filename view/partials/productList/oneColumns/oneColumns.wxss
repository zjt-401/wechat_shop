/*图片占350rpx  */

.oneColumnsBox{
  width: 690rpx;
  height: 530rpx;
  padding: 10rpx 30rpx 10rpx 30rpx;
  overflow: hidden;

}
.oneColumnsBoxImg{
  width: 100%;
    height: 426.42rpx;
  overflow: hidden;
  border-radius: 0.2em;
}
.oneColumnsBoxImg image{
  width: 100%;
  height: 100%;
}

.oneColumnsBoxName{

 
    text-align: left;
     margin-top: 10rpx;
     font-size: 25rpx;
       display: -webkit-box;
    word-break: break-all;
    text-overflow: ellipsis;
    overflow: hidden;
    -webkit-box-orient: vertical;
    -webkit-line-clamp:2;
}


/*--价格和购物车图片  */
.oneColumnsBoxMoney{
  
  width: 100%;
}
.oneColumnsBoxMoney text{
  font-size: 30rpx;
  display: block;
  
  color: red;
}