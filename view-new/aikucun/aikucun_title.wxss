/* aikucun_title */

.aikucun_title{
  padding: 34rpx 0 34rpx 0;
  display: flex;
  flex-direction: column;
  background: #fff;
  border-top-left-radius: 0.4em;
  border-top-right-radius: 0.4em;
}
.aikucun_title_tags{
  display: flex;
  justify-content: center;
  align-items: center;
}
.aikucun_title_container{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.leftLine{
  width: 1em;
  height: 1px;
  border-bottom: 2px solid #e24e24;
  margin: 0 1em;
}
.leftImage{
  width: 63.33333rpx;
  height: 17.3333rpx;
  margin: 0 17.3333rpx;
  line-height:17.3333rpx;
  margin-top: -6rpx;
}
.leftImage > image{
  width: 100%;
  height: 100%;
}
.border_style_sanjiao{
    width:0;
    height:0;
    border-top-width:6px;
    border-bottom-width: 6px;
    border-left-width: 3px;
    border-right-width: 3px;
    border-style:solid;
    margin-left: 10px;
    margin-right: 10px;
    margin-top: 5rpx;
}
.aikucun_title_text{
  color: #e24148;
  font-size: 1.1em;
  font-weight:bold;
}
.aikucun_title_num{
  color: #252525;
  font-size: 24rpx;
  text-align: center;
  margin-top: 4rpx;
}
