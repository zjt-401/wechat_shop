
/*列表*/


.flx_cenetr_content_title{
  padding: 10px;
  background: #f2f2f2;
  color: #aaa;
  margin:0 10px;
}
.flx_cenetr_content_item{
  border-top: 1px solid #ddd;
  padding: 10px;
  margin:0 10px;
  background: #fff;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.flx_cenetr_content_item image{
  width: 1em;
  height: 1em;
}
.flx_person_conut{
  padding: 0px 5px;
  background: red;
  color: #fff;
  border-radius: 4px;
  margin-left: 5px;
}

