
.headSearch{
  position: fixed;
  z-index:111;
  top: 0;
  width: 100%;
  height: 2.5em;
  background: #8B572A;
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  align-items: center;
}
.headSearch_input{
  background: #fff;
  width:65%;
  height: 80%;
  border-radius: 0.4em;
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  align-items: center;
}
.headSearch_input input{
  font-size: 0.7em;
  height: 100%;
  width: 95%;
}
.headSearch_icon{
   height: 80%;
  padding-left: 1em;
  padding-right: 1em;
  display: flex;
  justify-content: center;
  align-items: center;
}

.headSearch_view_height{
  width: 100%;
  height: 2.5em;
}




